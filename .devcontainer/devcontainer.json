{
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "containerUser": "dev",
  "remoteUser": "dev",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/dev/src,type=bind,consistency=cached",
  "workspaceFolder": "/home/dev/src",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "charliermarsh.ruff",
        "tamasfe.even-better-toml",
        "fill-labs.dependi"
      ]
    }
  }
}